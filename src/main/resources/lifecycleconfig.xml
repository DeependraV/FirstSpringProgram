<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

   <context:annotation-config/>




   <bean class="org.example.firstproject.LifeCycle.Samosa" name="samosa" init-method="init" destroy-method="destroy">
   <property name="price">
      <value>25</value>
   </property>
   </bean>

   <bean class="org.example.firstproject.LifeCycle.Pepsi" name="pepsi" >
      <property name="price">
         <value>30</value>
      </property>
   </bean>
   <bean class="org.example.firstproject.LifeCycle.AnnotationLifeCycle" name="ablc" >
      <property name="price">
         <value>30</value>
      </property>
   </bean>
   <!-- more bean definitions go here -->
</beans>